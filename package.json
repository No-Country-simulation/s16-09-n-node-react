{
  "name": "fullstack-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:frontend": "npm install --prefix frontend",
    "install:backend": "npm install --prefix backend",
    "install:all": "npm-run-all -p install:frontend install:backend",
    "start:backend": "npm run dev --prefix backend",
    "start:frontend": "npm run dev --prefix frontend",
    "start:all": "npm-run-all --parallel start:backend start:frontend",
    "build:backend": "npm run build --prefix backend",
    "build:frontend": "npm run build --prefix frontend",
    "build:all": "npm-run-all build:backend build:frontend",
    "db:seed": "npm run db:seed --prefix backend",
    "db:delete": "npm run db:delete --prefix backend"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
